aDisplay = stringifiedData.length > 35 ?\r\n            `${stringifiedData.substring(0, 35)}...`:\r\n            stringifiedData;\r\n\r\n        if (this.state.displayTransaction) {\r\n            return (\r\n                <div>\r\n                    <h3>Data: </h3>\r\n                    {\r\n                        data.map(transaction => (\r\n                            <div key={transaction.id}>\r\n                                <hr />\r\n                                <Transaction transaction={transaction} />\r\n                            </div>\r\n                        ))\r\n                    }\r\n                    <Button\r\n                        bsStyle='success'\r\n                        bsSize='small'\r\n                        onClick={this.toggleTransaction}\r\n                    >\r\n                        Show Less\r\n                    </Button>\r\n                </div>\r\n            )\r\n        }\r\n\r\n        return (\r\n            <div>\r\n                <h3>Data: </h3>\r\n                <p>{dataDisplay}</p>\r\n                <Button\r\n                    bsStyle='success'\r\n                    bsSize='small'\r\n                    onClick={this.toggleTransaction}\r\n                >\r\n                    Show More\r\n                </Button>\r\n            </div>\r\n        )\r\n    }\r\n\r\n    render() {\r\n        const { timestamp, hash } = this.props.block;\r\n\r\n\r\n        return (\r\n            <div>\r\n                <h3>Hash: </h3>\r\n                <p>{hash}</p>\r\n                <h3>Timestamp: </h3>\r\n                <p>{new Date(timestamp).toLocaleString()}</p>\r\n                {this.displayTransaction}\r\n            </div>\r\n        )\r\n    }\r\n}\r\n\r\nexport default Block;"},"lineCount":null}},"hash":"892cb44fbf5cba139cde54455bff94db","cacheData":{"env":{}}}